{
    "name": "yookue.github.io",
    "version": "0.1.0",
    "title": "Yookue Official Site",
    "description": "Awesome Open-Source Software Provider",
    "homepage": "https://yookue.github.io/official-site",
    "organization": "Yookue Ltd",
    "private": true,
    "license": "MIT",
    "keywords": [
        "yookue",
        "yookue-official-site"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "set NODE_OPTIONS=--openssl-legacy-provider && dumi dev",
        "build:watch": "father dev",
        "docs:build": "set NODE_OPTIONS=--openssl-legacy-provider && del-cli ./docs-dist && dumi build",
        "docs:publish": "gh-pages -d ./docs-dist"
    },
    "devDependencies": {
        "del-cli": "^6.0.0",
        "dumi": "^1.1.54",
        "typescript": "^4.9.5",
        "gh-pages": "^6.2.0"
    },
    "peerDependencies": {
        "typescript": ">= 4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yookue/official-site.git"
    },
    "bugs": {
        "url": "https://github.com/yookue/official-site/issues"
    },
    "browserslist": {
        "production": [
            "> 0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "author": {
        "name": "Yookue Ltd",
        "email": "yookue@outlook.com",
        "url": "https://yookue.com"
    },
    "contributors": [
